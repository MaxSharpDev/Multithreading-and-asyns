    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Multithreading_and_asyns</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="data\games.json" />
    <Content Remove="data\locations.json" />
